<div class="row">

    <form class="col s12" action="/registration" method="POST">

        <div class="row">
            <div class="input-field col s6">
                <input placeholder="Имя" id="first_name" type="text" class="validate" name="first_name">
                <label for="first_name"></label>
            </div>

            <div class="input-field col s6">
                <input placeholder="Фамилия" id="last_name" type="text" class="validate" name="last_name">
                <label for="last_name"></label>
            </div>
        </div>

        <div class="row">
            <div class="input-field col s12">
                <input placeholder="Login or username" type="text" class="validate" name="login">
                <label for="login"></label>
            </div>
        </div>

        <div class="row">
            <div class="input-field col s12">
                <input placeholder="Password" id="password" type="password" class="validate" name="password">
                <label for="password"></label>
            </div>
        </div>

        <div class="row">
            <div class="input-field col s12">
                <input placeholder="Email" id="email" type="email" class="validate">
                <label for="email"></label>
            </div>
        </div>

      
        <label style="font-size: large">Выберите роль</label>
        <p><label>
            <input name="role" type="radio" value="Client" checked />
            <span>Пользователь</span>
        </label></p>
        <p><label>
            <input name="role" type="radio" value="Staff"/>
            <span>Доставщик</span>
        </label></p>
        <p><label>
            <input name="role" type="radio" value="Administrator"/>
            <span>Администратор</span>
        </label></p>

       <button class="btn waves-effect waves-light" type="submit">Зарегистрироваться</button>

    </form>
  </div>